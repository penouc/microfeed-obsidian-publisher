{
  "name": "obsidian-microfeed-plugin",
  "version": "1.0.0",
  "description": "A powerful Obsidian plugin for publishing notes to Microfeed with automatic magazine-style image generation",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "microfeed",
    "publishing",
    "markdown",
    "cms",
    "podcast",
    "image-generation"
  ],
  "author": "Microfeed Community",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/obsidian-microfeed-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/obsidian-microfeed-plugin/issues"
  },
  "homepage": "https://github.com/your-username/obsidian-microfeed-plugin#readme",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "canvas": "^2.11.2",
    "html-to-image": "^1.11.11",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0"
  }
}
